SpinBox1*SpinBox1.initialDelay     : 5000
SpinBox1*SpinBox1.repeatDelay      : 5000

! Motif/CDE Convergence
SpinBox1*SpinBox1.arrowLayout      : ARROWS_FLAT_BEGINNING
